<element type="MiniMapWindow" style="MyWindow">
	<attribute name="Min Size" value="{4*iconX+2*spaceX} {4.5*iconY+3*spaceY}"/>
	<attribute name="Max Size" value="{4*iconX+2*spaceX} {4.5*iconY+3*spaceY}" />
	<attribute name="Vert Alignment" value="Bottom" />
	<attribute name="Horiz Alignment" value="Right" />
	<attribute name="Layout Mode" value="Vertical" />
	<attribute name="Layout Spacing" value="{spaceY}" />
	<attribute name="Layout Border" value="{spaceX} {spaceY} {spaceX} {spaceY}" />
</element>	

<element type="MiniMapIcon" style="Button">
	<attribute name="Min Size" value="{iconX*2/3-spaceX/2} {iconY/2-spaceY}" />
	<attribute name="Max Size" value="{iconX*2/3-spaceX/2} {iconY/2-spaceY}" />
</element>

<element type="MiniMapCheckBox" style="CheckBox" >
	<attribute name="Min Size" value="{iconX*2/3+spaceX} {iconY/2-spaceY}" />
	<attribute name="Max Size" value="{iconX*2/3+spaceX} {iconY/2-spaceY}" />
	<attribute name="Image Rect" value="192 80 208 96" />
	<attribute name="Is Checked" value="true" />
</element>

<element type="MiniMapListRow" style="ListRow" >
	<attribute name="Layout Spacing" value="{spaceX/2}" />
	<attribute name="Min Size" value="100 {iconY/2}" />
	<attribute name="Max Size" value="10000 {iconY/2}" />
</element>

<element type="MiniMapSprite">
	<attribute name="Vert Alignment" value="Center" />
	<attribute name="Horiz Alignment" value="Center" />
	<attribute name="My Size" value="{iconX*2/3+spaceX} {iconY/2-spaceY}" />
	<attribute name="Percent HotSpot" value="0.5 0.5" />
	<attribute name="Opacity" value="0.8" />
</element>

<element type="MiniMap" >
	<attribute name="Max Size" value="{iconX*3} {iconY*3}" />
</element>
