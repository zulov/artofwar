<element type="LeftMenuWindow" style="MyWindow">
	<attribute name="Min Size" value="{4*iconX+5*spaceX} {4*iconY+3*spaceY}"/>
	<attribute name="Max Size" value="{4*iconX+5*spaceX} {4*iconY+3*spaceY}"/>
	<attribute name="Vert Alignment" value="Bottom"/>
	<attribute name="Horiz Alignment" value="Left"/>
</element>

<element type="LeftMenuInfoPanel" style="MyWindow">
	<attribute name="Min Size" value="{3*iconX} {1*iconY+2*spaceY}"/>
	<attribute name="Max Size" value="{4*iconX+5*spaceX} {2*iconY+2*spaceY}"/>
	<attribute name="Vert Alignment" value="Bottom"/>
	<attribute name="Horiz Alignment" value="Left"/>
	<attribute name="Layout Mode" value="Vertical"/>
	<attribute name="Position" value="0 {-(4*iconY+3*spaceY)}"/>
	<attribute name="Layout Border" value="{spaceX} {spaceY} {spaceX} {spaceY}"/>
</element>

<element type="LeftMenuBigIcon" style="Button">
	<attribute name="Min Size" value="{iconX} {iconY}"/>
	<attribute name="Max Size" value="{iconX} {iconY}"/>
</element>

<element type="LeftMenuIcon" style="Button">
	<attribute name="Min Size" value="{iconX-4*spaceX} {iconY/2+spaceY}"/>
	<attribute name="Max Size" value="{iconX-4*spaceX} {iconY/2+spaceY}"/>
</element>

<element type="LeftMenuCheckBox" style="CheckBox">
	<attribute name="Min Size" value="{iconX+spaceX} {iconY/2+spaceY}"/>
	<attribute name="Max Size" value="{iconX+spaceX} {iconY/2+spaceY}"/>
	<attribute name="Image Rect" value="192 80 208 96"/>
	<attribute name="Is Checked" value="false"/>
</element>

<element type="LeftMenuListRow" style="ListRow">
	<attribute name="Layout Spacing" value="{spaceX}"/>
	<attribute name="Min Size" value="100 {iconY+spaceY}"/>
	<attribute name="Max Size" value="10000 {iconY+spaceY}"/>
</element>

<element type="LeftMenuSmallSprite">
	<attribute name="Vert Alignment" value="Center"/>
	<attribute name="Horiz Alignment" value="Center"/>
	<attribute name="My Size" value="{iconX+spaceX} {iconY/2+spaceY}"/>
	<attribute name="Percent HotSpot" value="0.5 0.5"/>
	<attribute name="Opacity" value="0.8"/>
</element>

<element type="LeftMenuSprite">
	<attribute name="Vert Alignment" value="Center"/>
	<attribute name="Horiz Alignment" value="Center"/>
	<attribute name="My Size" value="{iconX} {iconY}"/>
	<attribute name="Percent HotSpot" value="0.5 0.5"/>
	<attribute name="Opacity" value="0.8"/>
</element>

<element type="LeftMenuMock">
	<attribute name="Layout Spacing" value="{spaceY}"/>
	<attribute name="Layout Mode" value="Vertical"/>
	<attribute name="Vert Alignment" value="Bottom"/>
	<attribute name="Min Size" value="{iconX*3/4} {0}"/>
	<attribute name="Max Size" value="{4*iconX+5*spaceX} {4*iconY+3*spaceY}"/>
	<attribute name="Layout Border" value="{spaceX} {spaceY} {spaceX} {spaceY}"/>
</element>
